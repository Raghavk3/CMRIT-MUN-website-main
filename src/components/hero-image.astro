---
import { Picture } from "@astrojs/image/components";
import moonImage from "~/assets/footer.jpg";

const widths = [450, 800, 1500];
const sizes = "100vw";
---

<Picture
  class="h-full w-full object-cover"
  src={moonImage}
  widths={widths}
  sizes={sizes}
  alt="The ridged surface of the moon"
/>
